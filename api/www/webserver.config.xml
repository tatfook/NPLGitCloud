<?xml version="1.0" encoding="utf-8"?>
<WebServer>
    <servers>
        <server host="*" port="8181" host_state_name="">
            <defaultHost rules_id="api_rules"></defaultHost>
            <virtualhosts>
                <host name="www.sitename.com" rules_id="api_rules"></host>
            </virtualhosts>
        </server>
    </servers>
    <rules id="api_rules">
        <rule match="^.*$" with="WebServer.redirecthandler" params='{"/index.page"}'></rule>
    </rules>
</WebServer>
